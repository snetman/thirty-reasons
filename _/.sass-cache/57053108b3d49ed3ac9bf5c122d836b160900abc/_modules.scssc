@linei:@has_childrenT:@template"ì#/* Links as buttons
------------------------- */

a.btn {
    background-color: $vlt-gray;
    color: $gray;
    display: inline-block;
    padding: .35em .75em .3em;
    border-radius: 5px;
    font-size: 1em;
    line-height: 1.25em;
    font-weight: 500;
    font-style: normal;
    @include transit(background-color, .2s, linear);
    .ss-icon {
        position: relative;
        top: .15em;
    }
    &:hover {
        background-color: $white;
        color: $blue;
    }
}
a.btn-dark {
    @extend a.btn;
    color: $dk-gray;
    background-color: $lt-gray;
}
a.btn-blue {
    @extend a.btn;
    color: $white;
    background-color: $blue;
    &:hover {
        background-color: $vdk-gray;
        color: $white;
    }
}

.button-set {
    @extend .group;
    margin: 0;
    padding: 0;
    list-style: none;
    li {
        margin: 0;
        float: left;
        margin-right: 1px;
        }
    li:last-child {
        margin-right: 0;
    }
    a.btn {
        border-radius: 0;
    }   
    li:first-child a.btn {
        border-bottom-left-radius: 5px;
        border-top-left-radius: 5px;
    }
    li:last-child a.btn {
        border-bottom-right-radius: 5px;
        border-top-right-radius: 5px;
    }
}

/* Reason badges
-------------------------------------------------- */


/* Reason number
------------------------- */

.reason-number {
    font-size: 1.5em; // 24/16
    font-weight: 700;
    line-height: 1.25em; // 24/24
    padding: 0;
    color: $white;
    b {
        display: inline-block;
        margin: 0;
        padding: 0;
        width: 1em;
        border-radius: .2em;
        background-color: $blue;
        text-align: center;
        @include transit(background-color, .2s, linear);
        &:first-child {
            margin-right: .08333333em; /* 2px at base */
        }
    }
}

    
/* Reason name
------------------------- */

.reason-name {
    font-size: 1.5em; // 24/16
    font-weight: 700;
    color: $black;
    display: block;
    line-height: 1.25em; // 24/24
    margin: 0;
    padding: 0; // 3/24
    @include transit(color, .2s, linear);
    a {
        text-decoration: none;
    }    
}


/* Reason arrow
------------------------- */

.reason-arrow {
    font-size: 1.5em; // 24/16
    line-height: 1.25em;
    margin: 0;
    padding: 0;
    position: relative;
    color: $lt-gray;
    display: block;
    @include transit(color, .2s, linear);
    @include transit(left, .2s, ease-out);
}


/* Reason badge
------------------------- */

.reason-badge {
    .reason-number {
        float: left;
        margin-right: .25em;
    }
    .reason-name {
        float: left;
    }
    .reason-social {
        clear: both;
        font-size: .75em;
        padding-top: 1em;
    }
}

/* Reason logo
------------------------- */

.reason-logo {
    .reason-number {
        float: left;
        margin-right: .25em;
    }
    .reason-name {
        float: left;
        color: $blue;
    }
    .reason-social {
        clear: both;
        font-size: .75em;
        padding-top: 1em;
    }
    a {
      &:hover {
        .reason-number b {background-color: $black;}
        .reason-name {color: $black;}
      }
    }
}

/* Reason button
------------------------- */

.reason-button {
    .reason-arrow {
        float: left;
        margin-right: .25em;
        top: .125em;
    }
    .reason-number {
        float: left;
        margin-right: .25em;
        b {
            background-color: $gray;
        }
    }
    .reason-name {
        float: left;
        display: none;
        color: $gray;
        @media screen and (min-width: 480px) {
            display: block;
            font-size: 1em;
            clear: both;
            padding: .5em 0 0 1.75em;          
        }
        @media screen and (min-width: 760px) {
            clear: none;
            margin-right: .25em;
            padding: .1875em 0; // 3/16
            line-height: 1.5em; // 24/16
        }
        @media screen and (min-width: 1024px) {
            font-size: 1.5em;
            line-height: 1em;
            padding: .125em 0;
        }
        @media screen and (min-width: 1200px) {
            font-size: 1em;
            padding: .1875em 0; // 3/16
            line-height: 1.5em; // 24/16
        }
    }  
    a {
        @extend .group;
        display: block;
        margin: 0;
        padding: 1em .25em;
        background-color: $white; // $vlt-gray;
        @include transit(background-color, .2s, linear);
        &:hover {
            background-color: $white;
            .reason-number b {background-color: $blue;}
            .reason-name {color: $black;}
            .reason-arrow {color: $blue; left: -.25em;}
            }        
    }
}

/* Righty version */

.reason-button-righty {
    @extend .reason-button;
    .reason-arrow {
        float: right;
        margin-right: 0;
        margin-left: .25em;
    }
    .reason-number {
        float: right;
        margin-right: 0;
        margin-left: .25em;
    }
    .reason-name {
        float: right;
        // overriding the settings on the default config above for righty arrows
        @media screen and (min-width: 480px) {
            padding: .5em 1.75em 0 0;          
        }
        @media screen and (min-width: 760px) {
            float: right;
            margin-right: 0;
            margin-left: .25em;
            padding: .1875em 0; // 3/16
        }
        @media screen and (min-width: 1024px) {
            padding: .125em 0;
        }
        @media screen and (min-width: 1200px) {
            padding: .1875em 0; // 3/16
        }
    }
    a:hover .reason-arrow {left: .25em;}
}

/* Page title
-------------------------------------------------- */

.page-title {
  // any special page title styles go here.
  padding-bottom: $gutter;
}


/* Poster
------------------------- */

.poster {
    margin: 0;
    padding: 0;
    .image {
      position: relative;
      margin: 0;
      padding: 0;
      img {
        display: block;
        position: relative;
        outline: 1px solid $vlt-gray;
        @include transit(all, .15s, ease-out);
      }
    }
}

.poster-thumb {
  @extend .poster;
  .caption {
      font-size: .875em; // 14/16
      font-weight: 500;
      line-height: 1.14285em; // 16/14
      padding: .75em 0;
      border-bottom: 2px solid $lt-gray;
      height: 2.3em;
  }
  .image {
    max-with: 280px; // match size of image thumbnails
  }
  a:hover img {
    outline-color: $blue;
    box-shadow: 0 3px 16px rgba(0,0,0,.3);
  }
}

.poster-thumb-blank {
  @extend .poster-thumb;
  .image {
    img {
      background-color: $lt-gray;
      outline: 1px dashed $gray;
    }
  }
  .caption {
    color: $dk-gray;
  }
  .reason-number {
    font-size: 2em;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-left: -1em;
    margin-top: -.6em;
    z-index: 2;
    b {background-color: $dk-gray;}
  }
}

/* Poster grid */

.poster-grid {
  list-style: none;
  margin: 0;
  padding: 0;
  li {
    margin: 0 0 2*$gutter 0;
    @include grid-col(5);
  }
  @media screen and (min-width: 480px) {
    li {@include grid-col(2.5);}
  }
  @media screen and (min-width: 580px) {
    li {@include grid-col(5/3);}
  }  
  @media screen and (min-width: 760px) {
    li {@include grid-col(5/4);}
  }
  @media screen and (min-width: 1024px) {
    li {@include grid-col(1);}
  }
}


/* Tombstone
------------------------- */
.tombstone {
    font-size: .875em; // 14/16
    background-color: $vlt-gray;
    padding: 1.25em 1.5em 1.5em;
    border: 1px solid $vlt-gray;
    box-shadow: 0 3px 12px rgba(0,0,0,.25);
    .title {
      font-size: 1em; // 14/14
      font-weight: 700;
      line-height: 1.285em; // 18/14
      margin-bottom: .65em;
      color: $black;
    }
    .title + p {
        text-indent: 0;
    }
    p {
        font-size: 1em; // 14/14
        text-indent: 1.5em; // 18/14
        line-height: 1.285em;
        margin-bottom: 2.5714em; // 36/14
        color: $vdk-gray;
    }
    p + p {
        margin-top: -2.5714em;
    }
    .share {
        font-size: .85714em; // 12/14
        list-style: none;
        margin: 0;
        padding: 0;
        li {
            display: inline-block;
            margin-bottom: 0;
        }
    }           
}    


/* Prev / Next links
------------------------- */

.prev-next {
    @extend .group;
    list-style: none;
    margin: 0;
    padding: 0;
}
.prev-next > li {
    float: left;
    width: 50%;
    margin: 0;
    padding: 0;
    @include box-size;
    border: 2px solid $lt-gray;
    border-left-width: 0;
    border-right-width: 0;
    &:first-child {border-right-width: 2px;}
}

/* Sign-up form
-------------------------------------------------- */

#sign-up {
  label {color: $white;} 
}

/* Footer
-------------------------------------------------- */

#site-footer {
    .social {
        font-size: .75em;
        margin-bottom: 1em;
        }
    .copyright {
        font-size: .75em;
        color: $dk-gray;
    }    
    
}


/* Main navigation
-------------------------------------------------- */

.main-nav {
    display: none;
    padding: 0;
    margin: 0;
    list-style: none;
    > li {
        margin: 0;
    }
}




